,mBAAA,KAAA,CAAA,MAAA,CAAA,eAAA,CAAA,KAAA,KAAA,CAAA,MAAA,CAAA,GAAA,EAAA,GAAA;AACA;AACA;;AACA,eAAA,CAAA;AAAA;AACA,mBAAA,KAAA,CAAA,MAAA,CAAA,eAAA,CAAA,KAAA,KAAA,CAAA,QAAA,CAAA,GAAA,EAAA,GAAA;AACA;AACA;;AACA,eAAA,CAAA;AAAA;AACA,mBAAA,KAAA,CAAA,MAAA,CAAA,eAAA,CAAA,KAAA,KAAA,CAAA,UAAA,CAAA,GAAA,EAAA,GAAA;AACA;AACA;AAhBA;AAkBA,OAnBA,MAmBA;AACA;;;;;;;;;;;;;;;;;;;;AAqBA,aAAA,SAAA,CAAA,YAAA;AACA,kBAAA,KAAA;AACA,iBAAA,CAAA;AAAA;AACA,gBAAA,MAAA,CAAA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,CAAA,EAAA,CAAA,EAAA,GAAA;;AACA;AACA;;AACA,iBAAA,CAAA;AAAA;AACA,gBAAA,MAAA,CAAA,KAAA,CAAA,MAAA,CAAA,eAAA,CAAA,MAAA,CAAA,KAAA,CAAA,MAAA,CAAA,GAAA,EAAA,GAAA;;AACA;AACA;;AACA,iBAAA,CAAA;AAAA;AACA,gBAAA,MAAA,CAAA,KAAA,CAAA,MAAA,CAAA,eAAA,CAAA,MAAA,CAAA,KAAA,CAAA,QAAA,CAAA,GAAA,EAAA,GAAA;;AACA;AACA;;AACA,iBAAA,CAAA;AAAA;AACA,gBAAA,MAAA,CAAA,KAAA,CAAA,MAAA,CAAA,eAAA,CAAA,MAAA,CAAA,KAAA,CAAA,UAAA,CAAA,GAAA,EAAA,GAAA;;AACA;AACA;AAhBA;AAkBA,SAnBA;AAoBA;AAGA;;;;;;;;;;AASA;AAxFA,GAxGA;;AAkMA;;;AAGA,EAAA,SArMA,uBAqMA;AACA,SAAA,IAAA,CAAA,IAAA,CAAA,aAAA,EAAA,KAAA,kBAAA;AACA;AAvMA,CAAA","sourcesContent":["<template>\n  <div>\n      <div class=\"container\" id=\"main-wrap\" style=\"\">\n        <detail-nav-bar class=\"nav shadow-sm row\" @navClick=\"navClick\"></detail-nav-bar>\n        <!--Scroll-->\n        <scroll class=\"main-content\"\n                :probeType=\"3\"\n                :pull-up-load=\"true\"\n                ref=\"scroll\"\n                :listen-scroll=\"true\"\n                :pulldown=\"true\"\n                @getScrollPosition=\"getScrollPosition\">\n          <!--Swiper-->\n          <div class=\"row swiper\">\n            <div class=\"col p-0\">\n              <detail-swiper :topImages=\"topImages\"></detail-swiper>\n            </div>\n          </div>\n\n          <!--goodsBaseInfo-->\n          <detail-base-info :goods=\"goods\"></detail-base-info>\n\n          <!--goodsDetailInfo-->\n          <detail-shop-info :shop=\"shop\"></detail-shop-info>\n\n          <!--detailGoodsInfo-->\n          <detail-goods-info :detailInfo=\"detailGoodsInfo\" @imageLoaded=\"imageLoaded\"/>\n\n          <!--DetailGoodsParams-->\n          <detail-goods-params :params=\"params\" ref=\"params\"/>\n\n          <!--DetailComments-->\n          <detail-comments :comments=\"comments\" ref=\"comments\"/>\n\n          <!--GoodsList-->\n          <goods-list :goods=\"recommends\" ref=\"recommends\"/>\n        </scroll>\n      </div>\n    <!--BackTop-->\n    <back-top @click.native=\"backUpClick\"\n              v-show=\"showBackTop\"/>\n  </div>\n</template>\n\n<script>\n  /*child components*/\n  import DetailNavBar from \"./childComponents/DetailNavBar\";\n  import DetailSwiper from \"./childComponents/DetailSwiper\";\n  import DetailBaseInfo from \"./childComponents/DetailBaseInfo\";\n  import DetailShopInfo from \"./childComponents/DetailShopInfo\";\n  import DetailGoodsInfo from \"./childComponents/DetailGoodsInfo\";\n  import DetailGoodsParams from \"./childComponents/DetailGoodsParams\";\n  import DetailComments from \"./childComponents/DetailComments\";\n\n  /*network*/\n  import {getDetail,Goods,Shop,Params,Comments,getRecommend} from 'network/detail'\n\n  /*general components*/\n  import scroll from 'components/common/scroll/Scroll2'\n  import GoodsList from \"components/content/goodsList/GoodsList\";\n  import BackTop from \"components/content/backTop/BackTop\";\n\n  /*minxins*/\n  import {goodsListDebounce,backTop} from 'common/mixins'\n\n  export default {\n    name: \"Detail\",\n    components:{\n      DetailNavBar,\n      DetailSwiper,\n      DetailBaseInfo,\n      DetailShopInfo,\n      scroll,\n      DetailGoodsInfo,\n      DetailGoodsParams,\n      DetailComments,\n      GoodsList,\n      BackTop,\n    },\n    mixins:[\n      goodsListDebounce,\n      backTop,\n    ],\n    data(){\n      return{\n        id: null,\n        topImages: [],\n        goods:{},\n        shop:{},\n        detailGoodsInfo:{},\n        params:{},\n        comments:{},\n        recommends:[],\n        navY:[],\n        imageLoad:false,\n        nav:['params','comments','recommends']\n      }\n    },\n    created() {\n      //从路由中获取货物id\n      this.id = this.$route.query.id\n      /*\n      * 使用id从服务器获取数据\n      * */\n      getDetail(this.id).then((res)=>{\n       // console.log(res.result);\n        const data = res.result\n        /*\n        *轮播图\n        * */\n        this.topImages = data.itemInfo.topImages\n\n        /*\n        * 货物页面\n        * */\n        this.goods = new Goods(data.itemInfo,data.columns,data.shopInfo.services)\n\n        /*\n        * 商家简介\n        * */\n        this.shop = new Shop(data.shopInfo)\n\n        /*\n        * 物品详情\n        * */\n        this.detailGoodsInfo = {\n          detailInfo:data.detailInfo.desc,\n          detailImage:data.detailInfo.detailImage[0].list\n        }\n\n        /*\n        * 物品参数\n        * */\n        this.params = new Params(data.itemParams)\n\n        /*\n        * 评论\n        * */\n\n        this.comments = new Comments(this.data.rate.list)\n      })\n\n      /*\n      * 商品推荐\n      * */\n      getRecommend().then((res)=>{\n        //console.log(res.data.list);\n        this.recommends = res.data.list\n      })\n\n      this.$nextTick(()=>{\n\n\n      })\n\n    },\n    updated(){\n      this.$refs.scroll.refresh\n      this.navY = []\n      // console.log(this.$refs.recommends.$el);\n      this.navY.push(0)\n      this.navY.push(this.$refs.params.$el.offsetTop)\n      this.navY.push(this.$refs.comments.$el.offsetTop)\n      this.navY.push(this.$refs.recommends.$el.offsetTop)\n      // console.log(this.navY);\n    },\n    watch:{\n\n    },\n    methods:{\n      //图片加载完成后刷新scroll\n      imageLoaded(){\n        this.$refs.scroll.refresh\n        this.imageLoad = true\n      },\n      //决定回到顶部是否显示的高度\n      getScrollPosition(position){\n        this.showBackTop = (-position.y) > 1000;\n        //console.log(position.y);\n      },\n\n\n      navClick(index){\n        const time = 0\n        if(this.imageLoad === true){\n          switch (index) {\n            case 0:{\n              this.$refs.scroll.scrollTo(0,0,200)\n              break\n            }\n            case 1:{\n              this.$refs.scroll.scrollToElement(this.$refs.params.$el,200)\n              break\n            }\n            case 2:{\n              this.$refs.scroll.scrollToElement(this.$refs.comments.$el,200)\n              break\n            }\n            case 3:{\n              this.$refs.scroll.scrollToElement(this.$refs.recommends.$el,200)\n              break\n            }\n          }\n        }else {\n         /* setTimeout(()=>{\n            switch (index) {\n              case 0:{\n                this.$refs.scroll.scrollTo(0,0,200)\n                break\n              }\n              case 1:{\n                this.$refs.scroll.scrollToElement(this.$refs.params.$el,200)\n                break\n              }\n              case 2:{\n                this.$refs.scroll.scrollToElement(this.$refs.comments.$el,200)\n                break\n              }\n              case 3:{\n                this.$refs.scroll.scrollToElement(this.$refs.recommends.$el,200)\n                break\n              }\n            }\n          },300)*/\n\n         this.$nextTick(()=>{\n           switch (index) {\n             case 0:{\n               this.$refs.scroll.scrollTo(0,0,200)\n               break\n             }\n             case 1:{\n               this.$refs.scroll.scrollToElement(this.$refs.params.$el,200)\n               break\n             }\n             case 2:{\n               this.$refs.scroll.scrollToElement(this.$refs.comments.$el,200)\n               break\n             }\n             case 3:{\n               this.$refs.scroll.scrollToElement(this.$refs.recommends.$el,200)\n               break\n             }\n           }\n         })\n        }\n\n\n    /*    if(this.imageLoad === true){\n          console.log(this.$refs.scroll.scrollToElement);\n          this.$refs.scroll.scrollTo(0,-this.$refs.navY[index])\n        }else {\n          setTimeout(()=>{\n            this.$refs.scroll.refresh\n            this.$refs.scroll.scrollTo(0,-this.navY[index])\n          },3000)\n        }*/\n      }\n    },\n    /*\n    * 当离开detail时，停止对公共组件goosItem中imageLoaded的监听\n    * */\n    destroyed() {\n      this.$bus.$off('imageLoaded',this.goodsImageListener)\n    }\n  }\n</script>\n\n<style scoped>\n  #main-wrap{\n    background-color: white;\n    position: relative;\n    z-index: 101;\n    height: 100%;\n    font-weight: 200;\n  }\n  .nav{\n    position: relative;\n    z-index: 102;\n    background-color: white;\n  }\n  .main-content{\n    height: calc(100vh - 45px);\n  }\n</style>\n"],"sourceRoot":"src/views/detail"}]}